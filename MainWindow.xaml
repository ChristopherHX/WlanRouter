<Window x:Class="WlanRouter.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WlanRouter"
        mc:Ignorable="d"
        Title="MainWindow" Height="200" Width="300" ResizeMode="NoResize" Closing="Windows_closing">
    <Grid Height="171" VerticalAlignment="Bottom">
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Click="Button_click" Margin="10,141,0,0"/>
        <TextBox x:Name="SSID" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="Wifi" VerticalAlignment="Top" Width="120" Margin="162,9,0,0"/>
        <TextBox x:Name="Passwort" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="Passwort" VerticalAlignment="Top" Width="120" Margin="162,46,0,0"/>
        <TextBlock HorizontalAlignment="Left" TextWrapping="Wrap" Text="SSID" VerticalAlignment="Top" Margin="133,10,0,0"/>
        <TextBlock HorizontalAlignment="Left" TextWrapping="Wrap" Text="Passwort(min 8 Zeichen)" VerticalAlignment="Top" Margin="29,47,0,0"/>
    </Grid>
</Window>
