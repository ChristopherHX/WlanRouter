<Window x:Class="WlanRouter.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WlanRouter"
        mc:Ignorable="d"
        Title="Wlan Router 1.0" Height="150" Width="250" ResizeMode="NoResize" Closing="Window_Closing">
    <Grid>
        <Button x:Name="Starten_btn" Content="Starten" Margin="10,10,10,67" Click="Starten_Klick"/>
        <TextBlock x:Name="SSID_txt" HorizontalAlignment="Left" TextWrapping="Wrap" Text="SSID" VerticalAlignment="Bottom" Margin="10,0,0,45"/>
        <TextBlock x:Name="WPA_PSK_txt" HorizontalAlignment="Left" TextWrapping="Wrap" Text="WPA PSK" Margin="10,0,0,16" Height="16" VerticalAlignment="Bottom"/>
        <TextBox x:Name="SSID" Height="23" TextWrapping="Wrap" Text="Wlan" VerticalAlignment="Bottom" Margin="62,0,10,39"/>
        <TextBox x:Name="WPA_PSK" TextWrapping="Wrap" Text="Passwort" Margin="62,0,10,10" Height="23" VerticalAlignment="Bottom"/>
    </Grid>
</Window>
